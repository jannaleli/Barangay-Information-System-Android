<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_register_user"
        tools:context=".RegisterUser">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="211dp"
            android:layout_height="666dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintEnd_toStartOf="@+id/linearLayout5"
            app:layout_constraintBottom_toBottomOf="parent" android:id="@+id/linearLayout6"
            android:layout_marginLeft="24dp" android:layout_marginStart="24dp">
        <TextView
                android:text="Username"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView30"/>
        <EditText
                android:layout_width="189dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText"
        />
        <TextView
                android:text="Password"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView31"/>
        <EditText
                android:layout_width="183dp"
                android:layout_height="wrap_content"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/editText2"
        />
        <TextView
                android:text="First Name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView32"/>
        <EditText
                android:layout_width="185dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText3"
        />
        <TextView
                android:text="Last Name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView33"/>
        <EditText
                android:layout_width="194dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText10"
        />
        <TextView
                android:text="Email Address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView34"/>
        <EditText
                android:layout_width="194dp"
                android:layout_height="wrap_content"
                android:inputType="textEmailAddress"
                android:ems="10"
                android:id="@+id/editText4"
        />
        <TextView
                android:text="Phone Number"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView35"/>
        <EditText
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:inputType="phone"
                android:ems="10"
                android:id="@+id/editText5"
        />
        <TextView
                android:text="Birth Date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView36"/>
        <EditText
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="10"
                android:id="@+id/editText11"
        />
        <TextView
                android:text="Birth Place"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView37"/>
        <EditText
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText15"
        />
        <TextView
                android:text="Gender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView38"/>
        <EditText
                android:layout_width="196dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText36"
        />
        <Button
                android:text="Submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/submitRegisterButton"
        />
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="183dp"
            android:layout_height="642dp"
            android:id="@+id/linearLayout5" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintStart_toEndOf="@+id/linearLayout6" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="22dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="14dp"
            android:layout_marginRight="14dp">
        <TextView
                android:text="Civil Status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView39"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText32"
        />
        <TextView
                android:text="CTC No."
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView40"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText12"
        />
        <TextView
                android:text="TIN No."
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView41"/>
        <EditText
                android:layout_width="197dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText17"
        />
        <TextView
                android:text="Profession"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView43"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText16"
        />
        <TextView
                android:text="Weight"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView42"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText14"
        />
        <TextView
                android:text="Height"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView44"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/editText13"
        />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>